{
    "name": "brieucthomas/fop",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "symfony/symfony": "^3.2",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "twig/extensions": "^1.4",
        "symfony/swiftmailer-bundle": "^2.4",
        "symfony/monolog-bundle": "^3.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "openbuildings/postmark": "^0.3",
        "stof/doctrine-extensions-bundle": "^1.1",
        "sonata-project/intl-bundle": "^2.2",
        "jms/i18n-routing-bundle": "^2.0",
        "friendsofsymfony/user-bundle": "^2.0@dev",
        "ornicar/gravatar-bundle" : "^1.1",
        "knplabs/knp-menu-bundle": "^2.1",
        "brieucthomas/ergast-client": "^2.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "symfony/phpunit-bridge": "^3.2",
        "liip/functional-test-bundle": "^1.6",
        "doctrine/data-fixtures": "^1.2",
        "hautelook/alice-bundle": "^1.4"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml",
            "env-map": {
                "secret": "SYMFONY_SECRET",
                "current_season": "FOP_CURRENT_SEASON",
                "database_url": "DATABASE_URL",
                "mailer_host": "MAILER_HOST",
                "mailer_port": "MAILER_PORT",
                "mailer_user": "MAILER_USER",
                "mailer_password": "MAILER_PASSWORD",
                "mailer_auth_mode": "MAILER_AUTH_MODE",
                "mailer_encryption": "MAILER_ENCRYPTION"
            }
        }
    }
}
